<div class="cmsWrapper">
	<div class="releases">
		<div class="divHeader">Videos</div>
		<div class="tableWrapper">
			<div class="tableContent">
				<% if @videos.count > 0 %>
					<table width="100%">
						<tr class="tableHeader">
							<th>TITLE</th>
							<th>VIDEO URL</th>
						</tr>
						<tr><hr class="ruler"/></tr>
						<% @videos.each do |v| %>
							<tr class="tableRow">
								<td width="22%"><%= v.title %></td>
								<td width="72%"><%= v.url %></td>
								<!--<td width="3%">
									<input type="hidden" value="videos" class="modal-model-type">
									<input type="hidden" value=<%= v.id %> class="modal-object-id">
									<%= link_to image_tag('edit2.png'), '#myModal',  :data => {:toggle => "modal"}, class: "cmsEdit" %>
								</td>-->
								<td width="3%"><%= link_to image_tag('edit2.png'), edit_video_path(v) %></td>
								<td width="3%"><%= link_to image_tag('delete2.png'), v, :confirm => 'Are you sure you want to delete this video?', :method => :delete %></td>
							</tr>
						<% end %>
					</table>
				<% else %>
					<table width="100%"><tr class="tableRow empty"><td><em>YOU HAVE NO VIDEOS YET... ADD SOME BELOW ?!</em></td></tr></table>
				<% end %>
			</div>
		</div>
	</div>
	<h6>
	<input type="hidden" value="videos" class="modal-model-type">
	<%= link_to 'Add new video', '#myModal',  :data => {:toggle => "modal"}, class: "cmsAdd darkButton2"  %></h6>
</div>